{
  "cli": {
    "version": ">= 14.4.1",
    "appVersionSource": "remote"
    
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "store",
      "env": {
        "GOOGLE_SERVICE_INFO": "@env:GOOGLE_SERVICE_INFO",
        "USE_MODULAR_HEADERS": "1"
      }
    },
    "preview": {
      "distribution": "store",
      "env": {
        "GOOGLE_SERVICE_INFO": "@env:GOOGLE_SERVICE_INFO",
        "USE_MODULAR_HEADERS": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "environment": "production",
      "distribution": "store",
      "env": {
        "GOOGLE_SERVICE_INFO": "@env:GOOGLE_SERVICE_INFO",
        "USE_MODULAR_HEADERS": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "denish.subedi@icloud.com"
      }
    }
  }
}

